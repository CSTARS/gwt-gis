<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.spacer {
			width: 1px;
			height: 25px;
			border-left: 1px solid #cccccc;
			background-color: #eeeeee;
		}
	</ui:style>
	<g:HTMLPanel ui:field="root">	
		<table cellpadding="0" cellspacing="0" style="width:100%">
			<tr>
				<td valign="center" align="middle" style="width: 120px">
					<g:SimplePanel ui:field="basemapPanel" />
				</td>
				<td valign="center" align="left" style="width: 5px">
					<g:HTML styleName="{style.spacer}" >&nbsp;</g:HTML>
				</td>
				<td valign="center" align="left" id="iconPanelTd">
					<g:MenuBar ui:field="menu" />
				</td>
				<td valign="center" align="left" style="width: 5px">
					<g:HTML styleName="{style.spacer}" >&nbsp;</g:HTML>
				</td>
				<td valign="center" align="left">
					<g:HorizontalPanel ui:field="statusPanel"  spacing="4"/>
				</td>
				<td valign="center" align="left">
					<g:SimplePanel ui:field="messagePanel" />
				</td>
				<td valign="center" align="right">
					<g:SimplePanel ui:field="searchPanel" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 